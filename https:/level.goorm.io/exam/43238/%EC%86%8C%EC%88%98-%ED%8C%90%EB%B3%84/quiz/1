#include <stdio.h>
int PrimeNumber(int x);
int main()
{
	int A;
	
	scanf("%d", &A);
	
	if(PrimeNumber(A)==1)
	{
		printf("True");
	}
	else if(PrimeNumber(A)==0)
	{
		printf("False");
	}
	
}

int PrimeNumber(int x)
{
	for(int i = 2; i<=x; i++)
	{
		if(x%i==0)
		{
			if(x == i)
			{
				return 1;
			}
			else
			{
				return 0;
			}
		}
	}
	return 0;
}
